{
  "name": "web-projects",
  "version": "1.0.0",
  "description": "All my personal projects",
  "scripts": {
    "lint": "next lint",
    "prepare": "husky install",
    "dev": "next ./src/",
    "build": "rm -r ./src/out && next build ./src/ && next export ./src/ && npm run generate-sitemap && npm run generate-manifest && npm run robots-gen",
    "deploy": "npm run build && firebase deploy",
    "generate-sitemap": "node ./src/postbuild/sitemap-generator",
    "generate-manifest": "node ./src/postbuild/manifest-generator",
    "robots-gen": "node ./src/postbuild/robots-txt-generator",
    "start": "next start ./src/",
    "test-watch": "jest --watch",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ulisessg/web-projects.git"
  },
  "author": "Ulises SG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ulisessg/web-projects/issues"
  },
  "homepage": "https://github.com/Ulisessg/web-projects#readme",
  "devDependencies": {
    "@babel/preset-typescript": "7.16.7",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/styled-components": "5.1.20",
    "babel-jest": "27.4.6",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-plugin-react": "7.28.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "react-test-renderer": "17.0.2",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "axios": "0.25.0",
    "intersection-observer": "0.12.0",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-gist": "1.2.4",
    "styled-components": "5.3.3"
  }
}
