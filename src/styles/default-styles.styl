@import "./variables.styl"
*,
*:after,
*:before
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	margin: 0
	padding: 0
	list-style: none
	text-decoration: none
	border: none
	outline: none
	color: white
	font-family: 'Noto Sans', sans-serif
	overflow-x: hidden
body
	background-color: background-color