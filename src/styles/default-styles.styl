@import "./variables.styl"
*,
*:after,
*:before
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	margin: 0
	padding: 0
	list-style: none
	text-decoration: none
	outline: none
	font-family: 'Helvetica'
html
	font-size: 16px
body
	background-color: background-color

#root
	overflow: hidden
a
	font-size: unity + .5